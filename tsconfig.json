{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "es2022",
    "types": ["vite/client"],
    "isolatedModules": true,
    "moduleResolution": "Node",
    "module": "es2022",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
  },
  "include": ["src"]
}
